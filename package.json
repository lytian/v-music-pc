  "license": "MIT",
  "scripts": {
    "dev": "node scripts/watch.mjs",
    "prebuild": "vue-tsc --noEmit --p packages/renderer/tsconfig.json && node scripts/build.mjs",
    "build": "electron-builder",
    "lint": "eslint src/**/*.{js,jsx,vue,ts,tsx} --fix",
    "clean:lib": "rimraf node_modules",
    "clean:release": "rimraf release",
    "reinstall": "rimraf yarn.lock && rimraf package.lock.json && rimraf node_modules && cnpm install"
  },
  "engines": {
    "node": ">=14.17.0"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.5",
    "crypto-js": "^4.1.1",
    "floating-vue": "^2.0.0-beta.15",
    "lodash-es": "^4.17.21",
    "node-fetch": "^3.2.4",
    "vue-i18n": "^9.1.9"
  },
  "devDependencies": {
    "@intlify/vite-plugin-vue-i18n": "^3.4.0",
    "@types/lodash-es": "^4.17.6",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "@vitejs/plugin-vue": "^2.1.0",
    "electron": "18.0.2",
    "electron-builder": "^23.0.3",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.7.1",
    "less": "^4.1.2",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.3",
    "vite": "^2.9.1",
    "vite-plugin-electron": "^0.4.3",
    "vite-plugin-resolve": "^2.0.1",
    "vue": "^3.2.31",
    "vue-eslint-parser": "^8.3.0",
    "vue-router": "^4.0.14",
    "vue-tsc": "^0.31.1"
  },
  "env": {
    "VITE_DEV_SERVER_HOST": "127.0.0.1",
    "VITE_DEV_SERVER_PORT": 3000
  },
  "keywords": [
    "electron",
    "vite",
    "vue3",
    "music"
  ]
